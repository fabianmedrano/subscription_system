@{
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] </title>
    <partial name="~/Views/Shared/Layout/_CssPartial.cshtml" />
</head>
<body>
    <header>
        <partial name="_NavPartial" />
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        <partial name="~/Views/Shared/_NotificationPartial.cshtml" />
    </div>
    <partial name="~/Views/Shared/Layout/_FooterPartial.cshtml" />
    @await RenderSectionAsync("Scripts", required: false)
    <partial name="~/Views/Shared/Layout/_JsPartial.cshtml" />
</body>
</html>
